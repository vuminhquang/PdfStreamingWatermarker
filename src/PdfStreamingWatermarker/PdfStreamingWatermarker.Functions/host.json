{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Exception"
      }
    },
    "logLevel": {
      "default": "Information",
      "PdfStreamingWatermarker.Functions": "Debug",
      "Function": "Information",
      "Host.Results": "Error"
    }
  },
  "functionTimeout": "00:10:00",
  "extensions": {
    "http": {
      "maxConcurrency": 100,
      "maxOutstandingRequests": 200,
      "dynamicThrottlesEnabled": true
    },
    "queues": {
      "maxPollingInterval": "00:00:02",
      "visibilityTimeout": "00:00:30",
      "batchSize": 16,
      "maxDequeueCount": 5,
      "newBatchThreshold": 8
    }
  }
} 